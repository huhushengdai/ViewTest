<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <com.blizzmi.ninegrid.NineGridImageView
        android:id="@+id/groudIcon1"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_margin="2dp"
        android:background="#eeeeee" />

    <com.blizzmi.ninegrid.NineGridImageView
        android:id="@+id/groudIcon2"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_margin="2dp"
        android:layout_toRightOf="@id/groudIcon1"
        android:background="#eeeeee" />

    <com.blizzmi.ninegrid.NineGridImageView
        android:id="@+id/groudIcon3"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_margin="2dp"
        android:layout_toRightOf="@id/groudIcon2"
        android:background="#eeeeee" />

    <com.blizzmi.ninegrid.NineGridImageView
        android:id="@+id/groudIcon4"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_below="@id/groudIcon1"
        android:layout_margin="2dp"
        android:background="#eeeeee" />

    <com.blizzmi.ninegrid.NineGridImageView
        android:id="@+id/groudIcon5"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_below="@id/groudIcon2"
        android:layout_margin="2dp"
        android:layout_toRightOf="@id/groudIcon4"
        android:background="#eeeeee" />

    <com.blizzmi.ninegrid.NineGridImageView
        android:id="@+id/groudIcon6"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_below="@id/groudIcon3"
        android:layout_margin="2dp"
        android:layout_toRightOf="@id/groudIcon5"
        android:background="#eeeeee" />

    <com.blizzmi.ninegrid.NineGridImageView
        android:id="@+id/groudIcon7"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_below="@id/groudIcon4"
        android:layout_margin="2dp"
        android:background="#eeeeee" />

    <com.blizzmi.ninegrid.NineGridImageView
        android:id="@+id/groudIcon8"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_below="@id/groudIcon5"
        android:layout_margin="2dp"
        android:layout_toRightOf="@id/groudIcon7"
        android:background="#eeeeee" />

    <com.blizzmi.ninegrid.NineGridImageView
        android:id="@+id/groudIcon9"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_below="@id/groudIcon6"
        android:layout_margin="2dp"
        android:layout_toRightOf="@id/groudIcon8"
        android:background="#eeeeee" />

    <!--<com.blizzmi.ninegrid.CircleImageView-->
        <!--android:layout_width="wrap_content"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:layout_alignParentBottom="true"-->
        <!--android:src="@mipmap/ic_launcher"-->
        <!--/>-->
</RelativeLayout>